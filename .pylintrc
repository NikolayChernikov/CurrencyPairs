[MASTER]
extension-pkg-allow-list =
    dependency_injector,
    pydantic,

[MESSAGES CONTROL]
disable =
    C0103, # invalid-name
    C0116, # missing-function-docstring
    R0801, # Similar lines in 2 files
    R0903, # too-few-public-methods
    W1203, # logging-fstring-interpolation

[FORMAT]
max-line-length=120
